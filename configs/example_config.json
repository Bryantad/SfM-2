{
    "architectures": [
        "GPT2LMHeadModel"
    ],
    "vocab_size": 16384,
    "n_positions": 2048,
    "n_ctx": 2048,
    "n_embd": 768,
    "n_layer": 12,
    "n_head": 12,
    "activation_function": "gelu_new",
    "resid_pdrop": 0.1,
    "embd_pdrop": 0.1,
    "attn_pdrop": 0.1,
    "layer_norm_epsilon": 1e-5,
    "initializer_range": 0.02,
    "bos_token_id": 1,
    "eos_token_id": 2,
    "pad_token_id": 0,
    "gradient_checkpointing": true,
    "use_cache": false,
    "summary": "SFM-2: GPT-2 style model for Sona code generation with robust syntax and 16K BPE vocab."
}